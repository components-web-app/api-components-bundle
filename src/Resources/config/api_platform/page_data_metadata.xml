<resources xmlns="https://api-platform.com/schema/metadata"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="https://api-platform.com/schema/metadata
           https://api-platform.com/schema/metadata/metadata-2.0.xsd">
    <resource class="Silverback\ApiComponentsBundle\Metadata\PageDataMetadata">
        <attribute name="normalizationContext">
            <attribute name="jsonld_embed_context">true</attribute>
        </attribute>
        <property name="resourceClass" identifier="true" readable="true" writable="false"/>
        <property name="properties" writable="false"/>
        <collectionOperations>
            <collectionOperation name="GET"/>
        </collectionOperations>
        <itemOperations>
            <itemOperation name="GET"/>
        </itemOperations>
    </resource>
</resources>
