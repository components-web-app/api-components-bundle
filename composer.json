{
    "name": "silverbackis/api-component-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-exif": "*",
        "ext-fileinfo": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "api-platform/api-pack": "^1.1",
        "api-platform/core": "^2.3.6",
        "cocur/slugify": "^3.0",
        "doctrine/common": "^2.7",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/doctrine-migrations-bundle": "^1.3",
        "doctrine/orm": "^2.5.11",
        "gesdinet/jwt-refresh-token-bundle": "^0.5.4",
        "guzzlehttp/guzzle": "^6.3",
        "jackalope/jackalope-doctrine-dbal": "^1.3",
        "lexik/jwt-authentication-bundle": "^2.6",
        "liip/imagine-bundle": "^2.0",
        "ocramius/proxy-manager": "^2.2",
        "ramsey/uuid": "^3.7",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/apache-pack": "^1.0",
        "symfony/browser-kit": "^4.2",
        "symfony/console": "^4.2",
        "symfony/doctrine-bridge": "^4.2",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/monolog-bridge": "^3.4",
        "symfony/monolog-bundle": "^3.3",
        "symfony/proxy-manager-bridge": "^4.2",
        "symfony/serializer": "^4.2",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/twig-bundle": "^4.2"
    },
    "require-dev": {
        "ext-imagick": "*",
        "behat/behat": "^3.1",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3.1",
        "behat/symfony2-extension": "^2.1.1",
        "behatch/contexts": "^3.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "infection/infection": "^0.11.0",
        "roave/security-advisories": "dev-master",
        "symfony/debug": "^4.2",
        "symfony/dotenv": "^4.2",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Silverback\\ApiComponentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Silverback\\ApiComponentBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
